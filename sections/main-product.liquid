<script>
(function () {
  // Resolve the product form element safely (handles snippet variations)
  function getProductForm() {
    // Prefer the explicit id if present
    var explicitId = {{ product_form_id | json }};
    if (explicitId) {
      var byId = document.getElementById(explicitId);
      if (byId) return byId;
    }
    // Fallbacks used by Dawn-style themes
    var el =
      document.querySelector('#ProductInfo-{{ section.id }} form[id^="product-form-"]') ||
      document.querySelector('product-form form[id^="product-form-"]') ||
      document.querySelector('form[action*="/cart/add"]');
    return el || null;
  }

  function placeNoThanks() {
    var form = getProductForm();
    if (!form) return;

    var addBtn = form.querySelector('button[name="add"], button[type="submit"]');
    var paymentWrap = form.closest('#ProductInfo-{{ section.id }}')
                      ?.querySelector('.shopify-payment-button');
    var noThanks = document.querySelector('.no-thanks-link');

    if (!noThanks || !addBtn) return;

    // Put "No thanks" right after Add to Cart
    if (addBtn.nextElementSibling !== noThanks) {
      addBtn.insertAdjacentElement('afterend', noThanks);
    }

    // If dynamic checkout exists, keep "No thanks" above it and inject note (once)
    if (paymentWrap) {
      if (paymentWrap.previousElementSibling !== noThanks) {
        paymentWrap.parentElement.insertBefore(noThanks, paymentWrap);
      }
      {% comment %} injectRecurringNote(paymentWrap); {% endcomment %}
    }
  }


  // Single global click handler for "No thanks"
  document.addEventListener('click', function (e) {
    var a = e.target.closest('.js-no-thanks');
    if (!a) return;
    e.preventDefault();
    if (document.referrer && window.history.length > 1) {
      window.history.back();
    } else {
      window.location.href = '{{ routes.root_url }}';
    }
  });

  // Initial placement + re-run when dynamic checkout renders
  document.addEventListener('DOMContentLoaded', placeNoThanks);
  var obs = new MutationObserver(placeNoThanks);
  obs.observe(document.body, { childList: true, subtree: true });
})();
</script>

<script>
(function () {
  var ROOT_SEL = '#ProductInfo-{{ section.id }}';

  function dedupeInside(root) {
    if (!root) return;

    // --- Dedupe "More payment options" / dynamic checkout ---
    var payBtns = root.querySelectorAll('.shopify-payment-button');
    if (payBtns.length > 1) {
      payBtns.forEach(function (el, i) {
        if (i > 0) el.remove();
      });
    }

    // --- Dedupe "No thanks" link ---
    var noThanks = root.querySelectorAll('.no-thanks-link');
    if (noThanks.length > 1) {
      noThanks.forEach(function (el, i) {
        if (i > 0) el.remove();
      });
    }
  }

  // Run on load and on any DOM changes under the section
  function runDedupe() {
    var root = document.querySelector(ROOT_SEL);
    dedupeInside(root);
  }

  document.addEventListener('DOMContentLoaded', runDedupe);
  new MutationObserver(runDedupe).observe(document.querySelector('body'), {
    childList: true,
    subtree: true
  });
})();
</script>







<script>
(function () {
  var banner = document.getElementById('pet-top-banner');
  if (!banner) return;

  var nameSpan = banner.querySelector('.pet-banner__name');

  function setBannerName(name) {
    var finalName = (name && String(name).trim()) ? name.trim() : ' Your Pet';
    nameSpan.textContent = finalName;
  }

  // Try to read from likely places (popup globals, storage, cookie, query)
  function getPetName() {
    try {
      // 1) If your popup exposes a global like window.PawPlan.pet.name
      if (window.PawPlan && window.PawPlan.pet && window.PawPlan.pet.name) {
        return window.PawPlan.pet.name;
      }
    } catch(e){}

    // 2) Local/session storage fallbacks
    var keys = ['pawplan_pet_name','pawplan.pet_name','pet_name'];
    for (var i=0; i<keys.length; i++) {
      var v = localStorage.getItem(keys[i]) || sessionStorage.getItem(keys[i]);
      if (v && v.trim()) return v.trim();
    }

    // 3) Cookie fallback
    var m = document.cookie.match(/(?:^|;)\s*pet_name=([^;]+)/);
    if (m) return decodeURIComponent(m[1]);

    // 4) Querystring fallback (?pet_name=Buddy)
    var qs = new URLSearchParams(location.search);
    if (qs.has('pet_name')) return qs.get('pet_name');

    return '';
  }

  // Update immediately with anything we have
  setBannerName(getPetName());

  // Listen for your popup to tell us the name (emit this when user submits/changes)
  // Example from popup JS: document.dispatchEvent(new CustomEvent('pawplan:pet-updated', { detail: { name: 'Buddy' } }));
  document.addEventListener('pawplan:pet-updated', function (ev) {
    var n = ev && ev.detail && ev.detail.name;
    if (n && n.trim()) {
      try { localStorage.setItem('pawplan_pet_name', n.trim()); } catch(e){}
      setBannerName(n.trim());
    }
  });

  // Optional: if your popup sets window.PawPlan.pet.name later, poll briefly to catch it once.
  var tries = 0;
  var poll = setInterval(function(){
    var nm = getPetName();
    if (nm) { setBannerName(nm); clearInterval(poll); }
    if (++tries > 20) clearInterval(poll); // stop after ~2s
  }, 100);
})();
</script>




<script>
(function () {
  // Adjust selectors if your popup lives elsewhere
  var nameInput = document.querySelector('#pet-name');

  function updateNameFromInput() {
    if (!nameInput) return;
    var n = (nameInput.value || '').trim();
    if (!n) return;

    try { localStorage.setItem('pawplan_pet_name', n); } catch(e) {}

    // Tell the product page banner to update immediately
    document.dispatchEvent(new CustomEvent('pawplan:pet-updated', {
      detail: { name: n }
    }));
  }

  if (nameInput) {
    // Update as they type
    nameInput.addEventListener('input', updateNameFromInput);

    // Also update on blur (covers autofill/paste)
    nameInput.addEventListener('change', updateNameFromInput);
  }

  // If popup opens with a prefilled value, push it once on load
  document.addEventListener('DOMContentLoaded', function () {
    if (nameInput && nameInput.value && nameInput.value.trim()) {
      updateNameFromInput();
    }
  });
})();
</script>






